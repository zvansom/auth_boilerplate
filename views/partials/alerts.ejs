<% if(alerts.success) { %>
  <% alerts.success.forEach( msg => { %>
    <div class="row">
      <div class="card-panel green">
        <%= msg %>
      </div>
    </div>
  <% }); %>
<% } %>

<% if(alerts.error) { %>
  <% alerts.error.forEach( msg => { %>
    <div class="row">
      <div class="card-panel red">
        <%= msg %>
      </div>
    </div>
  <% }); %>
<% } %>
